    "@nestjs/microservices": "^9.0.9",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/hapi__joi": "^17.1.9",
    "amqp-connection-manager": "^4.1.6",
    "amqplib": "^0.10.2",
    "aws-sdk": "^2.1225.0",
    "axios": "^1.1.3",
    "cache-manager": "^5.1.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "exceljs": "^4.3.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
